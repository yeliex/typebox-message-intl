{
  "name": "typebox-message-intl",
  "version": "1.0.0",
  "description": "typebox error messages i18n support",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "es",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepublishOnly": "./scripts/build.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "typebox",
    "typebox-intl",
    "typebox-i18n",
    "typebox-error",
    "typebox-message"
  ],
  "author": "yeliex <yeliex@yeliex.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@sinclair/typebox": "^0.32.10",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.104",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.11.5",
    "swc-register-esm": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.32.0"
  }
}
